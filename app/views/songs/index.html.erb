<% @songs.all.each do |song| %>
<table>
    <thead>
    <tr>
        <th>Title</th>
        <th>Released</th>
        <th>Release Date</th>
        <th>Artist Name</th>
        <th>Genre</th>
        <th>Update</th>
    </tr>
    </thead>
    <tbody>
    <tr>
        <td><%= song.title %></td>
        <td><%= song.released %></td>
        <td><%= song.release_year %></td>
        <td><%= song.artist_name %></td>
        <td><%= song.genre %></td>
        <td><%= link_to 'edit', edit_song_path(song) %> </td>
    </tr>
    </tbody>
<% end %>

</table>