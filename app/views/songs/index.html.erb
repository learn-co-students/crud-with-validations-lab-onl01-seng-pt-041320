<table>
    <thead>
        <tr>
            <tr>Title</tr>
            <tr>Artist Name</tr>
            <tr>Genre</tr>
            <tr>Release Year</tr>
        </tr>
    </thead>
    <tbody>
        <% @songs.each do |song| %>
            <tr>
                <td><%= song.title %></td>
                <td><%= song.artist_name %></td>
                <td><%= song.genre %></td>
                <td><%= song.release_year %></td>
            </tr>
            <%= link_to 'View', song_path(song) %>
            <%= link_to 'Edit', edit_song_path(song) %>
            <%= link_to 'Delete', song %>
        <% end %>
    </tbody>
</table>