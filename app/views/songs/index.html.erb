<table>
  <thead>
    <tr>
    <th>Title</th>
            <th>Released</th>
            <th>Release_year</th>
            <th>artist_name</th>
            <th>genre</th>
    </tr>
  </thead>
    
<tbody>

<h1>Heres some songs</h1>
  <%= @songs.each do |song| %>
    <tr>
      <p ><%= song.title %></p>
      <p><%= song.released %></p>
      <p><%= song.release_year %></p>
      <p><%= song.artist_name %></p>    
      <p><%= song.genre %></p> 
      <%= link_to "Edit", edit_song_path(song) %>
  
    </tr>
  <% end %>
</thead>
</tbody>
</table
  