<table>
  <thead>
  <tr>
    <th>Title</th>
    <th>Released</th>
    <th>Release Year</th>
    <th>Artist Name</th>
    <th>Genre</th>
  </tr>
</thead>
<tbody>

<% @songs.each do |song| %>
<tr>
  <div class="song">
<td><%= song.title %><br><td>
  <%= link_to "Delete", song, method: :delete, data: { confirm: "Really?" } %>
</div>
<td><%= song.released %><br></td>
<td><%= song.release_year %><br></td>
<td><%= song.artist_name %><br></td>
<td><%= song.genre %><br></td>
</tr>
<% end %>
</tbody>
</table>
